{
  "name": "react_mentoring_homework",
  "version": "1.0.0",
  "description": "Homeworks for the React Mentoring Program",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open --hot --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js ",
    "test": "jest --watchAll",
    "lint": "eslint --fix src/**/*.js "
  },
